{
	"name": "ecommerce",
	"version": "1.0.0",
	"description": "MERN shopping cart app",
	"main": "server.js",
	"type": "module",
	"engines": {
		"node": "v16.6.1"
	},
	"scripts": {
		"start": "node backend/server",
		"server": "nodemon backend/server",
		"client": "npm start --prefix frontend",
		"dev": "concurrently \"npm run server\" \"npm run client\"",
		"data:import": "node backend/seeder",
		"data:destroy": "node backend/seeder -d",
		"build": "node server.js"
	},
	"author": "Joshua Vaughn",
	"license": "MIT",
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"colors": "^1.4.0",
		"dotenv": "^10.0.0",
		"escape-html": "^1.0.3",
		"express": "^4.17.1",
		"express-async-handler": "^1.1.4",
		"express-rate-limit": "^5.3.0",
		"jsonwebtoken": "^8.5.1",
		"mongodb": "^4.1.3",
		"mongoose": "^6.0.12",
		"morgan": "^1.10.0",
		"multer": "^1.4.3",
		"sanitize-filename": "^1.6.3",
		"concurrently": "^6.3.0",
		"mpath": "^0.8.4",
		"mquery": "^4.0.0",
		"nodemon": "^2.0.14"
	},
	"devDependencies": {
		"concurrently": "^6.3.0",
		"mpath": "^0.8.4",
		"mquery": "^4.0.0",
		"nodemon": "^2.0.14"
	}
}
